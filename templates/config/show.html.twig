{% extends 'base.html.twig' %}

{% block title %}Config{% endblock %}

{% block body %}
    <h1>Config</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ config.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ config.name }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ config.value }}</td>
            </tr>
            <tr>
                <th>Template_id</th>
                <td>{{ config.templateId }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ config.createdAt ? config.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ config.updatedAt ? config.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('config_index') }}">back to list</a>

    <a href="{{ path('config_edit', {'id': config.id}) }}">edit</a>

    {{ include('config/_delete_form.html.twig') }}
{% endblock %}
